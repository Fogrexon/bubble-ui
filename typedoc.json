  "entryPoints": ["./src/index.ts"],
  "tsconfig": "./tsconfig.json",
  "out": "./docs",
  "projectDocuments": [
    "./lib-docs/overview.md",
    "./lib-docs/build-process.md",
    "./lib-docs/quickstart.md",
